// 既定の設定とユーザー設定を上書きするには、このファイル内に設定を挿入します
{
        "search.exclude": 
    {
        "**/node_modules": true,
        "**/bower_components": true,
        "build/": true,
        "temp/": true,
        "library/": true,
        "**/*.anim": true
    },
    "files.exclude": 
    {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "library/": true,
        "local/": true,
        "temp/": true
    },

    "window.zoomLevel": 2,
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
}